<!DOCTYPE html>
<html lang="es">
<head>
	<title>Inicio</title>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
	<link rel="stylesheet" href="/css/main.css">
</head>
<body>
	<!-- SideBar -->
	{{#if user}}
	<section class="full-box cover dashboard-sideBar">
		<div class="full-box dashboard-sideBar-bg btn-menu-dashboard"></div>
		<div class="full-box dashboard-sideBar-ct">
			<!--SideBar Title -->
			<div class="full-box text-uppercase text-center text-titles dashboard-sideBar-title">
				ODINSON <i class="zmdi zmdi-close btn-menu-dashboard visible-xs"></i>
			</div>
			<!-- SideBar User info -->
			<div class="full-box dashboard-sideBar-UserInfo">
				<figure class="full-box">
					<img src="data:image/jpeg;base64," alt="monstruo_perfil">
					<figcaption class="text-center text-titles"></figcaption>
				</figure>
				<ul class="full-box list-unstyled text-center">
					<li>
						<a href="#!">
							<i class="zmdi zmdi-settings"></i>
						</a>
					</li>
					<li>
						<a href="/logout" class="btn-exit-system">
							<i class="zmdi zmdi-power"></i>
						</a>
					</li>
				</ul>
			</div>
			<!-- SideBar Menu -->
			<ul class="list-unstyled full-box dashboard-sideBar-Menu">
				<li>
					<a href="/">
						<i class="zmdi zmdi-view-dashboard zmdi-hc-fw"></i> Página principal
					</a>
				</li>
				
				<li>
					<a href="#!" class="btn-sideBar-SubMenu">
						<i class="zmdi zmdi-case zmdi-hc-fw"></i> Zona de Administracion <i class="zmdi zmdi-caret-down pull-right"></i>
					</a>
					<ul class="list-unstyled full-box">

						{{#ifCond user.__t '===' 'Administrador' '||' user.jefe_departamento }}
						<li>
							<a href="/ShowEtapasFP"><i class="zmdi zmdi-book zmdi-hc-fw"></i>Modificacion de Ciclos</a>
						</li>
						{{/ifCond}}

						{{#ifCond user.__t '===' 'Administrador' '||' user.jefe_departamento }}
						<li>
							<a href="/ShowEtapas"><i class="zmdi zmdi-book zmdi-hc-fw"></i>Modificacion de Etapas</a>
						</li>
						{{/ifCond}}

						{{#ifCond user.__t '===' 'Administrador' '||' user.jefe_departamento }}
						<li>
							<a href="/empresas"><i class="zmdi zmdi-book zmdi-hc-fw"></i>Modificacion de empresas</a>
						</li>
						{{/ifCond}}

						{{#ifCond user.__t '===' 'Administrador' '||' user.jefe_departamento }}
						<li>
							<a href="/festivos"><i class="zmdi zmdi-book zmdi-hc-fw"></i>Modificacion de Festivos</a>
						</li>
						{{/ifCond}}

						{{#ifCond user.tipoDisciplina '===' 'FP' '&&' user.tutor.curso '===' '2' }}
						<li>
							<a href="/alumnos_FCT"><i class="zmdi zmdi-font zmdi-hc-fw"></i>Modificacion de FCT</a>
						</li>
						{{/ifCond}}
					</ul>
				</li>
				

				<li>
					<a href="#!" class="btn-sideBar-SubMenu">
						<i class="zmdi zmdi-case zmdi-hc-fw"></i>Datos de interes<i class="zmdi zmdi-caret-down pull-right"></i>
					</a>
					<ul class="list-unstyled full-box">
						<li>
							<a href="/calendario"><i class="zmdi zmdi-timer zmdi-hc-fw"></i>Calendario</a>
						</li>
						<li>
							<a href="/horario"><i class="zmdi zmdi-timer zmdi-hc-fw"></i>Horario</a>
						</li>

     				{{#ifCond user.tipoDisciplina '===' 'FP'}}
						<li>
							<a href="/ShowAsignaturasForProfesor"><i class="zmdi zmdi-font zmdi-hc-fw"></i>Notas FP</a>
						</li>
					{{/ifCond}}

					{{#ifCond user.tipoDisciplina '===' 'FP' '&&' user.tutor.curso }}
						<li>
							<a href="/ShowClaseFP"><i class="zmdi zmdi-font zmdi-hc-fw"></i>Notas Curso FP</a>
						</li>
					{{/ifCond}}

					{{#ifCond user.tipoDisciplina '===' 'Etapa'}}
						<li>
							<a href="/ShowAsignaturasForProfesor_ETAPA"><i class="zmdi zmdi-font zmdi-hc-fw"></i>Notas ETAPA</a>
						</li>
					{{/ifCond}}

					{{#ifCond user.tipoDisciplina '===' 'Etapa' '&&' user.tutor.curso}}
						<li>
							<a href="/ShowClase_ETAPA"><i class="zmdi zmdi-font zmdi-hc-fw"></i>Notas Curso ETAPA </a>
						</li>
					{{/ifCond}}

					</ul>
				</li>

				<li>
					<a href="#!" class="btn-sideBar-SubMenu">
						<i class="zmdi zmdi-account-add zmdi-hc-fw"></i> Conjunto Educativo <i class="zmdi zmdi-caret-down pull-right"></i>
					</a>
					<ul class="list-unstyled full-box">

					{{#ifCond user.__t '===' 'Administrador' '||' user.jefe_departamento }}
						<li>
							<a href="/alumnos"><i class="zmdi zmdi-account zmdi-hc-fw"></i> Estudiantes</a>
						</li>
					{{/ifCond}}

					{{#ifCond user.__t '===' 'Administrador' '||' user.jefe_departamento }}
						<li>
							<a href="/profesores"><i class="zmdi zmdi-male-alt zmdi-hc-fw"></i>Profesores</a>
						</li>
					{{/ifCond}}
					</ul>
				</li>
			

				<li>
					<a href="#!" class="btn-sideBar-SubMenu">
						<i class="zmdi zmdi-account-add zmdi-hc-fw"></i> Estudiantes <i class="zmdi zmdi-caret-down pull-right"></i>
					</a>
				<ul class="list-unstyled full-box">
					{{#ifCond user.tipoDisciplina '===' 'Etapa' '&&' user.tutor.curso }}
						<li>
							<a href="alumnosETAPA"><i class="zmdi zmdi-face zmdi-hc-fw"></i>E.S.O y Bachillerato</a>
						</li>
					{{/ifCond}}

					 {{#ifCond user.tipoDisciplina '===' 'FP' '&&' user.tutor.curso }}
						<li>
							<a href="alumnosFP"><i class="zmdi zmdi-face zmdi-hc-fw"></i>FP Medio y Superior</a>
						</li>
					{{/ifCond}}

						{{#ifCond user.tipoDisciplina '===' 'FP' '&&' user.tutor.curso '===' '2' }}
						<li>
							<a href="/alumnos_FCT"><i class="zmdi zmdi-face zmdi-hc-fw"></i>Alumnos en FCT</a>
						</li>
						{{/ifCond}}
				</ul>
					
				</li>


				<li>
					<a href="#" class="btn-sideBar-SubMenu">
						<i class="zmdi zmdi-card zmdi-hc-fw"></i> Registro <i class="zmdi zmdi-caret-down pull-right"></i>
					</a>
					<ul class="list-unstyled full-box">

					{{#ifCond user.tipoDisciplina '===' 'FP' '&&' user.tutor.curso}}
						<li>
							<a href="/crear_alumnoFP"><i class="zmdi zmdi-money-box zmdi-hc-fw"></i> Registro estudiantes FP</a>
						</li>
					{{/ifCond}}

					{{#ifCond user.tipoDisciplina '===' 'Etapa' '&&' user.tutor.curso }}
						<li>
							<a href="/crear_alumno_ETAPA"><i class="zmdi zmdi-money-box zmdi-hc-fw"></i> Registro estudiantes Etapa</a>
						</li>
					{{/ifCond}}

						{{#ifCond user.__t '===' 'Administrador' '||' user.jefe_departamento }}
						<li>
							<a href="/crear_profesor"><i class="zmdi zmdi-money-box zmdi-hc-fw"></i> Registro profesor</a>
						</li>
						{{/ifCond}}

						{{#ifCond user.__t '===' 'Administrador' '||' user.jefe_departamento }}
						<li>
							<a href="/registrar_empresa"><i class="zmdi zmdi-money-box zmdi-hc-fw"></i> Registro empresa</a>
						</li>
						{{/ifCond}}


						{{#ifCond user.tipoDisciplina '===' 'FP' '&&' user.tutor.curso '===' '2' }}
						<li>
							<a href="/registrar_FCT"><i class="zmdi zmdi-money-box zmdi-hc-fw"></i> Registro FCT</a>
						</li>
						{{/ifCond}}


						{{#ifCond user.__t '===' 'Administrador' '||' user.jefe_departamento }}
						<li>
							<a href="/registrar_festivo"><i class="zmdi zmdi-money-box zmdi-hc-fw"></i> Registro de Festivos</a>
						</li>
						{{/ifCond}}
					</ul>
				</li>
				<li>
					<a href="/documentosProfesorado" class="btn-sideBar-SubMenu">
						<i class="zmdi zmdi-shield-security zmdi-hc-fw"></i>Documentos profesorado<i class="zmdi zmdi-caret-down pull-right"></i>
					</a>
					</ul>
				</li>
			</ul>
		</div>
	</section>
	{{/if}}
	<!-- Content page-->
	<section class="full-box dashboard-contentPage">
		<!-- NavBar -->
		<nav class="full-box dashboard-Navbar">
			<ul class="full-box list-unstyled text-right">
				<li class="pull-left">
					<a href="#!" class="btn-menu-dashboard"><i class="zmdi zmdi-more-vert"></i></a>
				</li>
				<li>
					<a href="#!" class="btn-Notifications-area">
						<i class="zmdi zmdi-notifications-none"></i>
						<span class="badge"></span>
					</a>
				</li>
				<li>
					<a href="#!" class="btn-search">
						<i class="zmdi zmdi-search"></i>
					</a>
				</li>
				<li>
					<a href="#!" class="btn-modal-help">
						<i class="zmdi zmdi-help-outline"></i>
					</a>
				</li>
			</ul>
		</nav>
    	{{{body}}}
	</section>
	<!-- Notificaciones -->
	<section class="full-box Notifications-area">
		<div class="full-box Notifications-bg btn-Notifications-area"></div>
		<div class="full-box Notifications-body">
			<div class="Notifications-body-title text-titles text-center">
				Notificaciones
			</div>

		</div>
	</section>

	<!-- Dialogo de ayuda-->
	<div class="modal fade" tabindex="-1" role="dialog" id="Dialog-Help">
		<div class="modal-dialog" role="document">
			<div class="modal-content">
				<div class="modal-header">
					<button type="button" class="close" data-dismiss="modal" aria-label="Close"><span
							aria-hidden="true">&times;</span></button>
					<h4 class="modal-title">Ayuda</h4>
				</div>
				<div class="modal-body">
					<p>
						Eres más torpe que un gusano con guantes
					</p>
				</div>
				<div class="modal-footer">
					<button type="button" class="btn btn-primary btn-raised" data-dismiss="modal"><i
							class="zmdi zmdi-thumb-up"></i> Ok</button>
				</div>
			</div>
		</div>
	</div>
	<!--====== Scripts -->
	<script src="/js/jquery-3.1.1.min.js"></script>
	<script src="/js/sweetalert2.min.js"></script>
	<script src="/js/bootstrap.min.js"></script>
	<script src="/js/material.min.js"></script>
	<script src="/js/ripples.min.js"></script>
	<script src="/js/jquery.mCustomScrollbar.concat.min.js"></script>
	<script src="/js/main.js"></script>
	<script>
		$.material.init();
	</script>
</body>
</html>